                <xsd:element type="xsd:integer" name="CUSTOMER_ID"/>
                <xsd:element name="BATCHES">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element name="BATCH">
                                <xsd:complexType>
                                    <xsd:sequence>
                                        <xsd:element type="xsd:integer" name="BANK_BATCH_ID"/>
