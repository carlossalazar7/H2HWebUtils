<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema">
    <xsd:element name="consultaLotesCliente">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element type="xsd:integer" name="CUSTOMER_ID"/>
                <xsd:element  name="BEGIN_DATE">
                    <xsd:complexType>
                        <xsd:simpleContent>
                            <xsd:extension base="xsd:date"></xsd:extension>
                        </xsd:simpleContent>
                    </xsd:complexType>
                </xsd:element>
                
                <xsd:element  name="END_DATE">
                    <xsd:complexType>
                        <xsd:simpleContent>
                            <xsd:extension base="xsd:date"></xsd:extension>
                        </xsd:simpleContent>
                    </xsd:complexType>
                </xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
</xsd:schema>